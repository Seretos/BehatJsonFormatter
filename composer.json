{
  "name": "seretos/BehatJsonFormatter",
  "description": "this library extends behat with an json formatter and publish commands to manage this generated json files",
  "keywords": [
    "behat",
    "json",
    "formatter"
  ],
  "homepage": "https://github.com/Seretos/BehatJsonFormatter",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arne von Appen",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/console":"^3.2",
    "symfony/dependency-injection":"^3.2",
    "symfony/http-kernel":"^3.2",
    "symfony/config":"^3.2",
    "symfony/yaml":"^3.2",
    "symfony/finder":"^3.2",
    "behat/behat":"^3.3",
    "behat/mink-extension":"^2.2",
    "behat/mink-selenium2-driver":"^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "seretos\\BehatJsonFormatter\\": "src/"
    }
  },
  "bin": [
    "bin/behat-json"
  ]
}